{
  "name": "template-fast",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.1.3",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.5",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/template": "^7.20.7",
    "@gatsbyjs/parcel-namer-relative-to-cwd": "^2.9.0",
    "assert": "^2.0.0",
    "buffer": "^5.7.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.38.0",
    "eslint-config-google": "^0.14.0",
    "nodemon": "^2.0.21",
    "parcel": "^2.8.3",
    "parcel-namer-rewrite": "^2.0.0-rc.3"
  },
  "scripts": {
    "start": "set NODE_ENV=development&& functions-framework --source=./src/main.js --target=main",
    "watch": "concurrently \"tsc -w\" \"nodemon --watch ./ --exec npm run start\"",
    "build-controller": "set NODE_ENV=development&& parcel build src/controllers/*/main.js --no-source-maps",
    "build-root": "set NODE_ENV=development&& parcel build src/main.js --no-source-maps",
    "build": "npm run build-controller && npm run build-root"
  },
